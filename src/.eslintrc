{
  "extends": ["@alexbainter/eslint-config/react"],
  "parserOptions": {
    "ecmaVersion": 2020
  },
  "globals": {
    "process": "readonly"
  },
  "overrides": [
    {
      "files": ["**/*.spec.js"],
      "env": {
        "mocha": true
      },
      "globals": {
        "expect": "readonly"
      }
    }
  ],
  "rules": {
    "react/prop-types": [2, { "ignore": ["data-cy"] }]
  }
}
